		<typeAlias alias="volReq" type="donate7.model.VolReq"/>
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/Member.xml" />
		<mapper resource="mybatis/Organ.xml" />
		<mapper resource="mybatis/Gift.xml" />
		<mapper resource="mybatis/Cpoint_info.xml" />
