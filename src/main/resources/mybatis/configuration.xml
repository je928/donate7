<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="jdbc.properties"></properties>
	<typeAliases>
		<typeAlias alias="member" type="donate7.model.Member" />
		<typeAlias alias="organ" type="donate7.model.Organ" />
		<typeAlias alias="gift" type="donate7.model.Gift" />
		<typeAlias alias="cpoint_info" type="donate7.model.Cpoint_info" />
