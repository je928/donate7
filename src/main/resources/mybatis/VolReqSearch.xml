<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="volReqSearch">
	<!-- Use type aliases to avoid typing the full classname every time. -->
	<resultMap id="VolReqSearchResult" type="volReqSearch">
		<id property="vt_No" column="VT_NO" />
		<result property="vt_Name" column="VT_NAME" />
		<result property="vt_Reg_O_No" column="VT_REQ_O_NO" />
		<result property="vt_Reg_Date" column="VT_REQ_DATE" />
		<result property="vt_Subject" column="VT_SUBJECT" />
		<result property="vt_Total" column="VT_TOTAL" />
		<result property="vt_Field" column="VT_FIELD" />
		<result property="vt_Cycle" column="VT_CYCLE" />
		<result property="vt_Start_Date" column="VT_START_DATE" />
		<result property="vt_End_Date" column="VT_END_DATE" />
		<result property="vt_Desc" column="VT_DESC" />
		<result property="vt_Mgr" column="VT_MGR" />
		<result property="vt_Mgr_Tel" column="VT_MGR_TEL" />
		<result property="vt_Mgr_Email" column="VT_MGR_EMAIL" />
		<result property="vt_Age" column="VT_AGE" />
		<result property="vt_Gender" column="VT_GENDER" />
		<result property="vt_Qualify" column="VT_QUALIFY" />
		<result property="vt_Prior_Edu" column="VT_PRIOR_EDU" />
		<result property="vt_Recruit_Yn" column="VT_RECRUIT_YN"/>
		<result property="addr" column="ADDR"/>
		<result property="reqcount" column="REQCOUNT"/>
	</resultMap>

	<select id="searchVolReq" resultMap="VolReqSearchResult">
		SELECT * FROM (SELECT ROWNUM RN, A.* FROM(SELECT * FROM SEARCH_VOLREQ_V ORDER BY VT_NO DESC) A) 
		<!-- WHERE RN BETWEEN 1 AND 2 -->
	</select>
</mapper>